#include <stdio.h>
#include <stdlib.h>

int main()
{
   int i,n,sum=0;
   int a[10];
   printf("enter the size of the element \n");
   scanf("%d",&n);
   printf("enter the array..\n");

   for(i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }

  for(i=0;i<n;i++)
  {
      sum=sum+a[i];
  }

  printf("sum of elements is %d",sum);
  return 0;

}
