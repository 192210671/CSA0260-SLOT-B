#include<stdio.h>
#include<conio.h>
int main()
{
    int n,m,i,j;
    int a[10][10],b[10][10];

    printf("enter no of rows,,");
    scanf("%d",&n);

    printf("enter the no of columns");

    scanf("%d",&m);

    printf("enter the elements in te matrix..\n");

    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
    {
        scanf("%d",&a[i][j]);
    }

    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
    {
        if(i==j)
            printf("%2d",a[i][j]);
            

    }
    return 0;
}
